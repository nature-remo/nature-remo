{
  "name": "nature-remo",
  "description": "Nature Remo API client",
  "version": "1.2.2",
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "scripts": {
    "build": "shx rm -rf ./lib && tsc",
    "docs": "yarn build && typedoc --mode file --out docs --excludeExternals  --excludePrivate --exclude '**/__tests__/*' src && touch ./docs/.nojekyll",
    "format": "prettier 'src/**/*.ts' --write",
    "lint": "tslint --project ./tsconfig.json --fix",
    "prepare": "npm run build",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.0.6",
    "codacy-coverage": "^3.4.0",
    "husky": "^4.2.1",
    "jest": "25.5.4",
    "lint-staged": "^10.0.7",
    "nock": "^12.0.2",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.0",
    "shipjs": "0.19.0",
    "shx": "^0.3.2",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "typedoc": "^0.17.3",
    "typescript": "^3.7.4"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uetchy/nature-remo.git"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test"
    }
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uetchy/nature-remo/issues"
  },
  "engines": {
    "node": ">=8.14.3"
  },
  "homepage": "https://github.com/uetchy/nature-remo",
  "keywords": [
    "nature-remo",
    "smart-home"
  ]
}
