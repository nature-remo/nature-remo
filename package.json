{
  "name": "nature-remo",
  "description": "Nature Remo API client",
  "version": "1.0.3",
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "shx rm -r ./lib && tsc",
    "docs": "yarn build && typedoc --mode file --out docs --excludeExternals  --excludePrivate --exclude '**/__tests__/*' src && touch ./docs/.nojekyll",
    "format": "prettier 'src/**/*.ts' --write",
    "lint": "tslint --project ./tsconfig.json --fix",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.0.4",
    "codacy-coverage": "^3.4.0",
    "husky": "^3.0.0",
    "jest": "24.9.0",
    "lint-staged": "^9.0.0",
    "nock": "^11.3.3",
    "prettier": "^1.17.1",
    "pretty-quick": "^2.0.0",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.1"
  },
  "bugs": {
    "url": "https://github.com/uetchy/nature-remo/issues"
  },
  "engines": {
    "node": ">=8.14.3"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "homepage": "https://github.com/uetchy/nature-remo",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test"
    }
  },
  "keywords": [
    "nature-remo",
    "smart-home"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uetchy/nature-remo.git"
  }
}
